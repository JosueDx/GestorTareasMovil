<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
      android:background="@drawable/f2"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.gestortareasapp.Eficiencia_tarea" >

        <TextView
            android:id="@+id/textViewNombreList"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="78dp"
            android:text="Eficiencia por Tarea"
            android:textColor="#6aba02"
            android:textSize="30dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textViewNombreList"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="45dp"
            android:text="Buscar Tarea"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ba5202" />

        <Button
            android:id="@+id/Button02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textViewNombreList"
            android:layout_below="@+id/textView1"
            android:layout_marginLeft="21dp"
            android:layout_marginTop="38dp"
            android:text="BUSCAR"
            android:textColor="#ba5202" />

        <EditText
            android:id="@+id/editText1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Button02"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="26dp"
            android:ems="10" >

            <requestFocus />
        </EditText>

        <Button
            android:id="@+id/Button01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/textViewNombreList"
            android:layout_below="@+id/editText1"
            android:layout_marginTop="37dp"
            android:text="CERRAR"
            android:textColor="#ba5202" />

</RelativeLayout>
